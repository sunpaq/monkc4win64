#include "HiWin.h"

oninit(HiWin)
{
	if(init(MCObject)) {
		obj->name = "";
		return obj;
	}
	return null;
}

method(HiWin, HiWin*, initWithName, const char* name)
{
	obj->name = name;
	return obj;
}

method(HiWin, void, sayHello, voida)
{
	printf("Hello windows!");
}

onload(HiWin)
{
	if(load(MCObject)) {
		binding(HiWin, void, sayHello, voida);
		return cla;
	}
	return null;
}
